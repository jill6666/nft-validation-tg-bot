{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/index.ts"
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/api", "permanent": false },
    {
      "source": "tonconnect-manifest.json",
      "destination": "./tonconnect-manifest.json",
      "permanent": false
    }
  ]
}
